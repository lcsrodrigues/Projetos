@model AgendaContatosWeb.ViewModels.NovoContatoViewModel
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
    List<AgendaContatosWeb.ViewModels.NovoContatoViewModel> listaContatos = new List<AgendaContatosWeb.ViewModels.NovoContatoViewModel>();
    listaContatos = ViewBag.listaContatos;
}

@Styles.Render("~/Assets/css/_contato.css")

<content>
    <ul>
        @foreach (AgendaContatosWeb.ViewModels.NovoContatoViewModel contatoVM in listaContatos)
        {

            <li class="card" userId="@contatoVM.Id">
                <div class="sideTop">
                    <div class="perfil">
                        <img src="data:image/jpeg;base64," />
                    </div>
                    <div class="actions">
                        <a href="@Url.Action("EditarContato","Contato", new { id = contatoVM.Id })"><img src="~/Assets/images/btnEdit.svg" title="Editar contato" /></a>
                        <a href="@Url.Action("DeletarContato","Contato", new { id = contatoVM.Id })"><img src="~/Assets/images/btnDelete.svg" title="Excluir contato" /></a>
                    </div>
                </div>
                <div>
                    <span class="txtNome">@contatoVM.Nome</span>
                </div>
                <div>
                    <span>@contatoVM.Telefone</span>
                </div>
                <div>
                    <span>@contatoVM.Email</span>
                </div>
            </li>
        }
    </ul>
</content>